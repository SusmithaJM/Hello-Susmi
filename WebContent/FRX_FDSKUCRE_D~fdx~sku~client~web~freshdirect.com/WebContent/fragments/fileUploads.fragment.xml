	</HBox>
	<VBox class="VBox">
		<Table>
			<columns>
				<Column>
					<Label text="File Name" design="Bold"/>
				</Column>
				<Column>
					<Label text="File Type" design="Bold"/>
				</Column>
				<Column>
					<Label text="Uploaded By" design="Bold"/>
				</Column>
				<Column>
					<Label text="Uploaded on" design="Bold"/>
				</Column>
				<Column>
					<Label text="Notes" design="Bold"/>
				</Column>
				<Column></Column>
			</columns>
			<items>
				<ColumnListItem>
					<Text text="a"/>
					<Select selectedKey="" items="{oDropdownModel>/fileType}" class="IconTabSelectClass">
					<core:Item key="{oDropdownModel>key}" text="{oDropdownModel>value}"/></Select>
					<Text text="a"/>
					<Text text="a"/>
					<Text text="a"/>
					<Button icon="sap-icon://delete" type="Reject"/>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</core:FragmentDefinition>