<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" class="sapUiSizeCompact">
	<HBox justifyContent="End">
		<Label text="File type:" design="Bold" style="margin-top:15%"/>
		<Select selectedKey="" class="IconTabSelectClass sapUiTinyMarginTop">
			<core:Item key="" text=""/>
		</Select>
		<Input value="" class="fileUploadInputStyle"/>
		<Button text="Browse" type="Accept" class="commitBtnClass"/>
