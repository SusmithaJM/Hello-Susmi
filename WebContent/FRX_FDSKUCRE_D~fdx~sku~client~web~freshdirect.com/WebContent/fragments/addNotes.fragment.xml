<core:FragmentDefinition xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns="sap.m" xmlns:core="sap.ui.core">
	<Popover title="Notes" placement="Top">
		<content>
			<Input value="{skuTempObjectModel>/notes}" />
		</content>
		<Toolbar class="sapUiSizeCompact">
			<ToolbarSpacer />
			<core:Icon src="sap-icon://accept" press="onAddNotes"/>
			<core:Icon src="sap-icon://decline" press="onCancelNotes"/>
		</Toolbar>
	</Popover>
</core:FragmentDefinition>